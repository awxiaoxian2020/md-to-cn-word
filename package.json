{
  "name": "md-to-cn-word",
  "version": "0.2.2",
  "description": "将Markdown转换为符合中国大陆地区惯用的初始风格的Word文档的库",
  "main": "dist/md-to-cn-word.umd.js",
  "module": "dist/md-to-cn-word.esm.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/md-to-cn-word.esm.js",
      "browser": "./dist/md-to-cn-word.umd.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "keywords": [
    "markdown",
    "docx",
    "converter",
    "chinese-style",
    "library"
  ],
  "author": "Xavi Lee",
  "homepage": "https://github.com/awxiaoxian2020/md-to-cn-word",
  "license": "LGPL-3.0-or-later",
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "html-to-docx": "^1.8.0",
    "inline-css": "^4.0.3",
    "showdown": "^2.1.0"
  },
  "browser": {
    "./dist/md-to-cn-word.umd.js": "./dist/md-to-cn-word.umd.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.36.0"
  }
}
